<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>AFP Procurement Service</title>
    <!-- font -->
    <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:300,400,600,700,800,900" rel="stylesheet">
    <link rel="stylesheet" href="static/assets/fonts/nucleo-webfonts/mini/css/nucleo-mini.css">
    <link rel="stylesheet" href="static/assets/fonts/nucleo-webfonts/glyph/css/nucleo-glyph.css">
    <link rel="stylesheet" href="static/assets/fonts/nucleo-webfonts/outline/css/nucleo-outline.css">

    <!-- main style -->
    <link rel="stylesheet" href="static/assets/css/main.css">
    <!-- scripts -->
    <script src="static/assets/js/vendor/modernizr-2.8.3.min.js"></script>

    <% if (htmlWebpackPlugin.options.nodeModules) { %>
      <!-- Add `node_modules/` to global paths so `require` works properly in development -->
      <script>
        require('module').globalPaths.push('<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>')
      </script>
    <% } %>
  </head>
  <body>
    <!-- <script src="static/assets/db/db.js"></script> -->
  <!--   <script type="text/javascript">
      console.log(dbPath)
    </script> -->
    <div id="app"></div>
    <!-- Set `__static` path to static files in production -->
    <script>
      if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(/\\/g, '\\\\')
    </script>
    <!-- Insert this line above script imports  -->
    <script>window.$ = window.jQuery = require('jquery');</script>
    <script>require('malihu-custom-scrollbar-plugin')($);</script>

    <script src="static/assets/js/main.js"></script>
    <script src="static/assets/js/plugins.js"></script>
    <script type="text/javascript">
        //close modal
        $('.modal__close-button').click(function(){
            $('.modal').removeClass('is-visible');
        })
    </script>

    <!-- webpack builds are automatically injected -->
  </body>
</html>
